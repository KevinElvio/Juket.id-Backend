{
    "version": 2,
    "builds": [
      {
        "src": "index.js",
        "use": "@vercel/node",
        "config": {
          "includeFiles": ["src/**"]
        }
      }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "/index.js"
      }
    ],
    "build": {
      "env": {
        "DATABASE_URL": "@database_url"
      },
      "commands": {
        "build": "npm run build"
      }
    }
  }